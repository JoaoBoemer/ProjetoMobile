        app:layout_constraintTop_toTopOf="@+id/radioButtonSab" />

    <TextView
        android:id="@+id/RadioDom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DOM"
        app:layout_constraintBottom_toTopOf="@+id/radioButtonDom"
        app:layout_constraintEnd_toEndOf="@+id/radioButtonDom"
        app:layout_constraintStart_toStartOf="@+id/radioButtonDom" />

    <!--app:layout_constraintEnd_toStartOf="@+id/TerTermino"-->
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/SegInicio" />

    <RadioButton
        android:id="@+id/radioButtonTer"
        android:layout_width="wrap_content"
