        app:layout_constraintStart_toEndOf="@+id/radioButtonQua"
        app:layout_constraintTop_toTopOf="@+id/radioButtonSex" />

    <TextView
        android:id="@+id/RadioSab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SAB"
        app:layout_constraintBottom_toTopOf="@+id/radioButtonSab"
        app:layout_constraintEnd_toEndOf="@+id/radioButtonSab"
