        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/RadioSab"
        app:layout_constraintEnd_toEndOf="@+id/InfoConsultorio"
        app:layout_constraintStart_toStartOf="@+id/InfoConsultorio"
        app:layout_constraintTop_toBottomOf="@+id/InfoConsultorio" />

